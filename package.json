{
  "name": "zotero-guide-zh",
  "version": "0.0.0",
  "description": "Zotero 中文手册",
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@8.11.0",
  "scripts": {
    "docs:dev": "vuepress dev src",
    "docs:clean-dev": "vuepress dev src --clean-cache",
    "docs:dev-debug": "vuepress dev src --debug",
    "docs:build": "vuepress build src",
    "lint:prettier": "prettier --write .",
    "lint:md": "markdownlint-cli2-fix src/**/*.md",
    "docs:update-package": "npx vp-update",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@vuepress/bundler-vite": "2.0.0-rc.7",
    "@vuepress/plugin-feed": "2.0.0-rc.12",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.2",
    "markdown-it-deflist": "^3.0.0",
    "markdownlint-cli2": "^0.10.0",
    "mermaid": "^10.8.0",
    "prettier": "2.8.8",
    "vue": "^3.4.16",
    "vuepress": "2.0.0-rc.7",
    "vuepress-plugin-search-pro": "2.0.0-rc.23",
    "vuepress-theme-hope": "2.0.0-rc.23"
  },
  "lint-staged": {
    "*.md": "prettier --write"
  }
}
